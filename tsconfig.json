{
	"compileOnSave": false,
	"name": "storm-react-diagrams",
	"compilerOptions": {
		"suppressExcessPropertyErrors": true,
		"declaration": true,
		"outDir": "dist/@types",
		"strictNullChecks": false,
		"sourceMap": true,
		"skipLibCheck": true,
		"jsx": "react",
		"baseUrl": ".",
		"paths": {
			"storm-react-diagrams": ["src/main.ts"]
		},
		"target": "es6",
		"module": "commonjs",
		"lib": [
			"dom",
			"es6"
		]
	},
	"include": [
		"./src"
	],
	"exclude": [
		"node_modules",
		"**/*.spec.ts",
		"./dist"
	]
}
